      "\u001b[0;34m\u001b[0m            \u001b[0mtuple\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m            \u001b[0mStrOptions\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0;34m{\u001b[0m\u001b[0;34m\"predict\"\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0;34m\"predict_proba\"\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0;34m\"decision_function\"\u001b[0m\u001b[0;34m}\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m        \u001b[0;34m]\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m        \u001b[0;34m\"greater_is_better\"\u001b[0m\u001b[0;34m:\u001b[0m \u001b[0;34m[\u001b[0m\u001b[0;34m\"boolean\"\u001b[0m\u001b[0;34m]\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m        \u001b[0;34m\"needs_proba\"\u001b[0m\u001b[0;34m:\u001b[0m \u001b[0;34m[\u001b[0m\u001b[0;34m\"boolean\"\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mHidden\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0mStrOptions\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0;34m{\u001b[0m\u001b[0;34m\"deprecated\"\u001b[0m\u001b[0;34m}\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m]\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m        \u001b[0;34m\"needs_threshold\"\u001b[0m\u001b[0;34m:\u001b[0m \u001b[0;34m[\u001b[0m\u001b[0;34m\"boolean\"\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mHidden\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0mStrOptions\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0;34m{\u001b[0m\u001b[0;34m\"deprecated\"\u001b[0m\u001b[0;34m}\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m]\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m    \u001b[0;34m}\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m    \u001b[0mprefer_skip_nested_validation\u001b[0m\u001b[0;34m=\u001b[0m\u001b[0;32mTrue\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\u001b[0;32mdef\u001b[0m \u001b[0mmake_scorer\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m    \u001b[0mscore_func\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m    \u001b[0;34m*\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m    \u001b[0mresponse_method\u001b[0m\u001b[0;34m=\u001b[0m\u001b[0;32mNone\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m    \u001b[0mgreater_is_better\u001b[0m\u001b[0;34m=\u001b[0m\u001b[0;32mTrue\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m    \u001b[0mneeds_proba\u001b[0m\u001b[0;34m=\u001b[0m\u001b[0;34m\"deprecated\"\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m    \u001b[0mneeds_threshold\u001b[0m\u001b[0;34m=\u001b[0m\u001b[0;34m\"deprecated\"\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m    \u001b[0;34m**\u001b[0m\u001b[0mkwargs\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m:\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m    \u001b[0;34m\"\"\"Make a scorer from a performance metric or loss function.\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m    A scorer is a wrapper around an arbitrary metric or loss function that is called\u001b[0m\n",
      "\u001b[0;34m    with the signature `scorer(estimator, X, y_true, **kwargs)`.\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m    It is accepted in all scikit-learn estimators or functions allowing a `scoring`\u001b[0m\n",
      "\u001b[0;34m    parameter.\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m    The parameter `response_method` allows to specify which method of the estimator\u001b[0m\n",
      "\u001b[0;34m    should be used to feed the scoring/loss function.\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m    Read more in the :ref:`User Guide <scoring>`.\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m    Parameters\u001b[0m\n",
      "\u001b[0;34m    ----------\u001b[0m\n",
      "\u001b[0;34m    score_func : callable\u001b[0m\n",
      "\u001b[0;34m        Score function (or loss function) with signature\u001b[0m\n",
      "\u001b[0;34m        ``score_func(y, y_pred, **kwargs)``.\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m    response_method : {\"predict_proba\", \"decision_function\", \"predict\"} or \\\u001b[0m\n",
      "\u001b[0;34m            list/tuple of such str, default=None\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m        Specifies the response method to use get prediction from an estimator\u001b[0m\n",
      "\u001b[0;34m        (i.e. :term:`predict_proba`, :term:`decision_function` or\u001b[0m\n",
      "\u001b[0;34m        :term:`predict`). Possible choices are:\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m        - if `str`, it corresponds to the name to the method to return;\u001b[0m\n",
      "\u001b[0;34m        - if a list or tuple of `str`, it provides the method names in order of\u001b[0m\n",
      "\u001b[0;34m          preference. The method returned corresponds to the first method in\u001b[0m\n",
      "\u001b[0;34m          the list and which is implemented by `estimator`.\u001b[0m\n",
      "\u001b[0;34m        - if `None`, it is equivalent to `\"predict\"`.\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m        .. versionadded:: 1.4\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m    greater_is_better : bool, default=True\u001b[0m\n",
      "\u001b[0;34m        Whether `score_func` is a score function (default), meaning high is\u001b[0m\n",
      "\u001b[0;34m        good, or a loss function, meaning low is good. In the latter case, the\u001b[0m\n",
      "\u001b[0;34m        scorer object will sign-flip the outcome of the `score_func`.\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m    needs_proba : bool, default=False\u001b[0m\n",
      "\u001b[0;34m        Whether `score_func` requires `predict_proba` to get probability\u001b[0m\n",
      "\u001b[0;34m        estimates out of a classifier.\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m        If True, for binary `y_true`, the score function is supposed to accept\u001b[0m\n",
      "\u001b[0;34m        a 1D `y_pred` (i.e., probability of the positive class, shape\u001b[0m\n",
      "\u001b[0;34m        `(n_samples,)`).\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m        .. deprecated:: 1.4\u001b[0m\n",
      "\u001b[0;34m           `needs_proba` is deprecated in version 1.4 and will be removed in\u001b[0m\n",
      "\u001b[0;34m           1.6. Use `response_method=\"predict_proba\"` instead.\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m    needs_threshold : bool, default=False\u001b[0m\n",
      "\u001b[0;34m        Whether `score_func` takes a continuous decision certainty.\u001b[0m\n",
      "\u001b[0;34m        This only works for binary classification using estimators that\u001b[0m\n",
      "\u001b[0;34m        have either a `decision_function` or `predict_proba` method.\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m        If True, for binary `y_true`, the score function is supposed to accept\u001b[0m\n",
      "\u001b[0;34m        a 1D `y_pred` (i.e., probability of the positive class or the decision\u001b[0m\n",
      "\u001b[0;34m        function, shape `(n_samples,)`).\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m        For example `average_precision` or the area under the roc curve\u001b[0m\n",
      "\u001b[0;34m        can not be computed using discrete predictions alone.\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m        .. deprecated:: 1.4\u001b[0m\n",
      "\u001b[0;34m           `needs_threshold` is deprecated in version 1.4 and will be removed\u001b[0m\n",
      "\u001b[0;34m           in 1.6. Use `response_method=(\"decision_function\", \"predict_proba\")`\u001b[0m\n",
      "\u001b[0;34m           instead to preserve the same behaviour.\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m    **kwargs : additional arguments\u001b[0m\n",
      "\u001b[0;34m        Additional parameters to be passed to `score_func`.\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m    Returns\u001b[0m\n",
      "\u001b[0;34m    -------\u001b[0m\n",
      "\u001b[0;34m    scorer : callable\u001b[0m\n",
      "\u001b[0;34m        Callable object that returns a scalar score; greater is better.\u001b[0m\n",
      "\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m    Examples\u001b[0m\n",
      "\u001b[0;34m    --------\u001b[0m\n",
      "\u001b[0;34m    >>> from sklearn.metrics import fbeta_score, make_scorer\u001b[0m\n",
      "\u001b[0;34m    >>> ftwo_scorer = make_scorer(fbeta_score, beta=2)\u001b[0m\n",
      "\u001b[0;34m    >>> ftwo_scorer\u001b[0m\n",
      "\u001b[0;34m    make_scorer(fbeta_score, response_method='predict', beta=2)\u001b[0m\n",
      "\u001b[0;34m    >>> from sklearn.model_selection import GridSearchCV\u001b[0m\n",
      "\u001b[0;34m    >>> from sklearn.svm import LinearSVC\u001b[0m\n",
      "\u001b[0;34m    >>> grid = GridSearchCV(LinearSVC(), param_grid={'C': [1, 10]},\u001b[0m\n",
      "\u001b[0;34m    ...                     scoring=ftwo_scorer)\u001b[0m\n",
      "\u001b[0;34m    \"\"\"\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m    \u001b[0mresponse_method\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0m_get_response_method\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m        \u001b[0mresponse_method\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mneeds_threshold\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mneeds_proba\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m    \u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m    \u001b[0msign\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0;36m1\u001b[0m \u001b[0;32mif\u001b[0m \u001b[0mgreater_is_better\u001b[0m \u001b[0;32melse\u001b[0m \u001b[0;34m-\u001b[0m\u001b[0;36m1\u001b[0m\u001b[0;34m\u001b[0m\n",
      "\u001b[0;34m\u001b[0m    \u001b[0;32mreturn\u001b[0m \u001b[0m_Scorer\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0mscore_func\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0msign\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mkwargs\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mresponse_method\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n",
      "\u001b[0;31mFile:\u001b[0m      /usr/local/lib/python3.10/dist-packages/sklearn/metrics/_scorer.py\n",
      "\u001b[0;31mType:\u001b[0m      function"
